@page
@using StreamsOfSound.Areas.Identity.Pages.Account.Manage
@model IndexModel
@{
    ViewData["ActivePage"] = ManageNavPages.Index;
}

<h3>@ViewData["Title"]</h3>
<partial name="_StatusMessage" for="StatusMessage" />
<div class="row">
    <div class="col-md-6">
        <form id="profile-form" method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <h3>Welcome to your profile. You can change your email and password. If you're a Volunteer, you can update your Instrument List as well.</h3>
        </form>
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
